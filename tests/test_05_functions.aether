// TEST 5: Functions and Calls
// Tests: function definitions, function calls, return values

func add(a: Int, b: Int) -> Int {
    a + b
}

func multiply(a: Int, b: Int) -> Int {
    a * b
}

func factorial(n: Int) -> Int {
    if n <= 1 { return 1 }
    n * factorial(n - 1)
}

func fibonacci(n: Int) -> Int {
    if n <= 1 { return n }
    fibonacci(n - 1) + fibonacci(n - 2)
}

func main() -> Int {
    // Test simple function calls
    let sum = add(3, 4)
    if sum != 7 { return 1 }
    
    let prod = multiply(5, 6)
    if prod != 30 { return 2 }
    
    // Test recursive factorial
    let fact5 = factorial(5)
    if fact5 != 120 { return 3 }
    
    // Test recursive fibonacci
    let fib10 = fibonacci(10)
    if fib10 != 55 { return 4 }
    
    // Print "OK"
    print(79)
    print(75)
    print(10)
    0
}

func print(c: Int) {
    __builtin_print(c)
}
