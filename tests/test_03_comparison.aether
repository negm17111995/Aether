// TEST 3: Comparison and Logic
// Tests: comparison operators, logical operators, if statements

func main() -> Int {
    let x = 10
    let y = 20
    
    // Less than
    if x < y {
        // OK
    } else {
        return 1
    }
    
    // Greater than
    if y > x {
        // OK
    } else {
        return 2
    }
    
    // Equal
    let z = 10
    if x == z {
        // OK
    } else {
        return 3
    }
    
    // Not equal
    if x != y {
        // OK
    } else {
        return 4
    }
    
    // Less than or equal
    if x <= 10 {
        // OK
    } else {
        return 5
    }
    
    // Greater than or equal
    if y >= 20 {
        // OK
    } else {
        return 6
    }
    
    // Print "OK"
    print(79)
    print(75)
    print(10)
    0
}

func print(c: Int) {
    __builtin_print(c)
}
