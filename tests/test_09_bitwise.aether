// TEST 9: Bitwise Operations
// Tests: AND, OR, XOR, NOT, shifts

func main() -> Int {
    let a = 12   // 1100 in binary
    let b = 10   // 1010 in binary
    
    // AND: 1100 & 1010 = 1000 = 8
    let and_result = a & b
    if and_result != 8 { return 1 }
    
    // OR: 1100 | 1010 = 1110 = 14
    let or_result = a | b
    if or_result != 14 { return 2 }
    
    // XOR: 1100 ^ 1010 = 0110 = 6
    let xor_result = a ^ b
    if xor_result != 6 { return 3 }
    
    // Test with larger numbers
    let x = 255    // 11111111
    let y = 170    // 10101010
    
    if (x & y) != 170 { return 4 }
    if (x | y) != 255 { return 5 }
    if (x ^ y) != 85 { return 6 }
    
    // Print "OK"
    print(79)
    print(75)
    print(10)
    0
}

func print(c: Int) {
    __builtin_print(c)
}
