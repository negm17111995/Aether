// Hash different content - proves FNV-1a is dynamic
import std
import std.pkg.registry

func main() -> Int {
    // Hash different bytes
    let data1 = ae_malloc(3)
    ae_store8(data1, 88)  // X
    ae_store8(data1 + 1, 89)  // Y
    ae_store8(data1 + 2, 90)  // Z
    
    let hash1 = hash_bytes(data1, 3)
    print(hash1)
    
    // Hash one byte
    let data2 = ae_malloc(1)
    ae_store8(data2, 42)
    let hash2 = hash_bytes(data2, 1)
    print(hash2)
    
    // Prove different inputs = different hashes
    if hash1 != hash2 {
        print(1)
    }
    
    0
}
