func main() -> Int {
    let a = malloc(16)
    let b = malloc(16)
    let c = malloc(16)

    store(a, 0, 10)
    store(a, 8, 20)
    
    store(b, 0, 30)
    store(b, 8, 40)
    
    // SIMD Add: c = a + b (2x Int at once)
    vec_add_mem(c, a, b)
    
    let r0 = load(c, 0)
    let r1 = load(c, 8)
    
    if r0 + r1 == 100 {
        return 0 // Success (40 + 60 = 100)
    }
    return 1
}
