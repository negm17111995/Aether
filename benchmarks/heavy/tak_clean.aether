// Tak Benchmark - Optimized for Bootstrap Compiler
// Uses decimal literals only

func tak(x: Int, y: Int, z: Int) -> Int {
    if y >= x {
        z
    } else {
        tak(
            tak(x - 1, y, z),
            tak(y - 1, z, x),
            tak(z - 1, x, y)
        )
    }
}

func main() -> Int {
    tak(30, 20, 10)
}
